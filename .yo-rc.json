{
  "generator-nikita": {
    "name": "nikita-generated-slim-build-folders",
    "private": true,
    "template": "slim",
    "sassCompiler": "libSass",
    "features": [
      "webfonts",
      "svgBackgrounds",
      "gitinfos"
    ],
    "nikitaCssMixins": [
      "respond-to"
    ],
    "nikitaCssExtends": [
      "cf"
    ],
    "formFramework": false,
    "useBuildFolders": true,
    "cleanBuildFolders": true,
    "requirejsPathToNodeModules": "../../node_modules/",
    "staticPageGenerator": "twigRender",
    "askBuildFolders": true
  }
}